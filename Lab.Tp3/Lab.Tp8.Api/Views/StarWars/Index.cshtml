@model List<Lab.Tp7.Common.RepoModels.PersonModel>
@{
    ViewBag.Title = "Characters Page";
}

<h3>Listado Personajes Star Wars:</h3>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Altura</th>
            <th scope="col">Color de ojos</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var person in Model)
        {
            <tr>
                <td>@person.Name</td>
                <td>@person.Height</td>
                <td>@person.EyeColor</td>
            </tr>
        }
    </tbody>
</table>